            document.getElementById(id).style.width = "0%";
        }</script>

    <script>function loadSvg() {
            fetch('/assets/images/rpt2.svg')
                .then(response => response.text())
                .then(svgText => {
                    const parser = new DOMParser();
                    const svgDoc = parser.parseFromString(svgText, "image/svg+xml");

                    const svgElement = document.getElementById('svgElement');
                    svgElement.innerHTML = '';
                    svgElement.appendChild(svgDoc.documentElement);

                    // Adjust the viewBox to zoom in or enlarge the SVG content
                    svgElement.setAttribute('viewBox', '0 0 700 700'); // Experiment with smaller values for more zoom
                })
                .catch(error => console.error('Error loading SVG:', error));
        }

        loadSvg();</script>


